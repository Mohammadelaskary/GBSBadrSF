<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <CheckBox
        android:id="@+id/defectname_checkBox"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="1"
        android:textSize="16sp"
        android:padding="8dp"
        style="@style/styleoftext"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/qcnotes"
        android:layout_width="130dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/defectname_checkBox"
        app:layout_constraintTop_toTopOf="@id/defectname_checkBox"
        app:layout_constraintEnd_toStartOf="@+id/productionnotes"
        android:layout_marginStart="100dp"
        android:text="@string/childdesc_txt"
        android:textSize="16sp"
        style="@style/styleoftext"
        />
    <TextView
        android:id="@+id/productionnotes"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/qcnotes"
        app:layout_constraintEnd_toStartOf="@id/productionrepair"
        app:layout_constraintTop_toTopOf="@id/qcnotes"
        android:text="250"
        android:layout_marginStart="50dp"
        android:textSize="16sp"
        style="@style/styleoftext"
        android:layout_marginTop="10dp"
        />
    <TextView
        android:id="@+id/productionrepair"
        android:layout_width="370dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/qcnotes"
        app:layout_constraintStart_toEndOf="@+id/productionnotes"
        android:layout_marginStart="40dp"
        android:layout_marginTop="10dp"
        android:text="ok"
        android:textSize="16sp"
        style="@style/styleoftext"
        />


</androidx.constraintlayout.widget.ConstraintLayout>